<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
  <template id="report_purchasequotation_document"
            inherit_id="purchase.report_purchasequotation_document"
  >
    <xpath expr="//div[@class='page']" position="replace">
      <div class="page">
        <table style="width:100%;">
          <tr>
            <th colspan="2">
              CENTROS DE COSTO
            </th>
          </tr>
          <t t-foreach="o.get_analytic_accounts()" t-as="aa">
            <tr>
              <td style="width:20%;">
                <span t-field="aa.code"/>
              </td>
              <td style="width:30%;">
                <span t-field="aa.name"/>
              </td>
            </tr>
          </t>
        </table>
      </div>
    </xpath>
  </template>
</odoo>