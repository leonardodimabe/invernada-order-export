<odoo>
  <data>
    <record model="ir.ui.view" id="stock_move_operations_form_inherit">
      <field name="name">formulario de movimientos</field>
      <field name="model">stock.move</field>
      <field name="inherit_id" ref="stock.view_stock_move_operations"/>
      <field name="arch" type="xml">
        <xpath expr="//footer" position="inside">
          <field name="has_serial_generated" invisible="1"/>
          <field name="has_tracking" invisible="1"/>
          <button name="generate_serial"
                  string="Generar CÃ³digos"
                  type="object"
                  attrs="{'invisible':['|',('has_serial_generated', '=', True),('has_tracking', 'not in', ['serial', 'lot'])]}"
          />
        </xpath>
      </field>
    </record>
    <record model="ir.ui.view" id="stock_move_operations_form_inherit">
        <field name="name">formulario movimientos</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_stock_move_operations" />
        <field name="arch" type="xml">
            <field name="move_line_ids" position="attributes">
                <attribute name="delete">0</attribute>
                <attribute name="create">0</attribute>
            </field>
        </field>
    </record>
  </data>
</odoo>